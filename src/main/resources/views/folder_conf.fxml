<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<fx:root prefHeight="200.0" prefWidth="380.0" type="GridPane" xmlns="http://javafx.com/javafx/8"
         xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label text="URL:"/>
        <Label text="Destination:" GridPane.rowIndex="1"/>
        <Label text="Source Folder:" GridPane.rowIndex="2"/>
        <Label text="Username:" GridPane.rowIndex="3"/>
        <Label text="Keyfile:" GridPane.rowIndex="4"/>
        <AnchorPane prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.columnSpan="2">
            <children>
                <TextField fx:id="url_text" layoutY="8.0" prefHeight="26.0" prefWidth="190.0"
                           AnchorPane.leftAnchor="8.0" AnchorPane.rightAnchor="75.0" AnchorPane.topAnchor="8.0"/>
                <Button fx:id="url_check" layoutX="204.0" layoutY="8.0" mnemonicParsing="false" onAction="#urlCheck"
                        text="Check" AnchorPane.leftAnchor="202.0" AnchorPane.rightAnchor="8.0"/>
            </children>
        </AnchorPane>
        <AnchorPane prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.columnSpan="2"
                    GridPane.rowIndex="1">
            <children>
                <ComboBox fx:id="destination_text" disable="true" layoutX="6.0" layoutY="8.0"
                          AnchorPane.bottomAnchor="8.0" AnchorPane.leftAnchor="8.0" AnchorPane.rightAnchor="8.0"
                          AnchorPane.topAnchor="8.0"/>
            </children>
        </AnchorPane>
        <AnchorPane prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.columnSpan="2"
                    GridPane.rowIndex="2">
            <children>
                <TextField fx:id="path_text" disable="true" layoutY="8.0" AnchorPane.leftAnchor="8.0"
                           AnchorPane.rightAnchor="75.0" AnchorPane.topAnchor="8.0"/>
                <Button fx:id="path_search" disable="true" layoutX="204.0" layoutY="8.0" mnemonicParsing="false"
                        onAction="#pathSearch" text="..." AnchorPane.leftAnchor="202.0" AnchorPane.rightAnchor="8.0"/>
            </children>
        </AnchorPane>
        <AnchorPane prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.columnSpan="2"
                    GridPane.rowIndex="4">
            <children>
                <TextField fx:id="keyfile_text" disable="true" layoutY="8.0" AnchorPane.leftAnchor="8.0"
                           AnchorPane.rightAnchor="75.0" AnchorPane.topAnchor="8.0"/>
                <Button fx:id="keyfile_search" disable="true" layoutX="204.0" layoutY="8.0" mnemonicParsing="false"
                        onAction="#keyFileSearch" text="..." AnchorPane.leftAnchor="202.0"
                        AnchorPane.rightAnchor="8.0"/>
            </children>
        </AnchorPane>
        <AnchorPane prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.columnSpan="2"
                    GridPane.rowIndex="3">
            <children>
                <TextField fx:id="username_text" disable="true" layoutX="14.0" layoutY="8.0"
                           AnchorPane.bottomAnchor="8.0" AnchorPane.leftAnchor="8.0" AnchorPane.rightAnchor="8.0"
                           AnchorPane.topAnchor="8.0"/>
            </children>
        </AnchorPane>
    </children>
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="181.0" minWidth="10.0"/>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="264.0" minWidth="10.0"/>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="264.0" minWidth="10.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
    </rowConstraints>
</fx:root>
